        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    var ga = new GlideAjax("GetUserCompanyAndKnowledgeBase");
    ga.addParam("sysparm_name", "getDataInternal");
    ga.addParam("company", g_form.getValue("company"));

    ga.getXMLAnswer(resultParse);
}

function resultParse(response) {
    if (response) {
        var result = JSON.parse(response);
        g_form.setValue("knowledge_base", result.knowledge_base);
    } else {
        g_form.setValue("knowledge_base", "");
    }
}]]></script>
