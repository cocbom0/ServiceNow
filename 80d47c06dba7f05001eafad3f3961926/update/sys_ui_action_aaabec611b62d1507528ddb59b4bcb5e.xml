        <client_script_v2><![CDATA[function onClick(g_form) {
	var ref = g_form.getDisplayValue('u_serial_number') || '';
	if(ref == '') {
		g_form.addInfoMessage('Please specify a serial number');
	} else {
		var array = ref.split("-");
		var item_tmp = array[1];
		var item = item_tmp.trim();	
		var url = 'https://partscatalog.liebherr.com/index.php/m/catalogSelection/index/identNo/' + item + '/search/1';
		top.window.open(url,'blank');
	}
}]]></client_script_v2>
