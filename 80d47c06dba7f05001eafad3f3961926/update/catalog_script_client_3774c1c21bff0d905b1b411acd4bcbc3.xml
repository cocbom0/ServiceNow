        <script><![CDATA[function onLoad() {
	var ga = new GlideAjax("GetUserCompanyAndKnowledgeBase");
	ga.addParam("sysparm_name", "getData");
	ga.addParam("sysparm_user_id", g_user.userID);
	ga.addParam("sysparm_start_category", "customer_portal");
	
	ga.getXMLAnswer(resultParse);
}

function resultParse(response) {
	if (response) {
		var result = JSON.parse(response);
		g_form.setValue("company", result.company);
		g_form.setValue("knowledge_base", result.knowledge_base);
		g_form.setValue("start_category", result.start_category);
	}
}]]></script>
